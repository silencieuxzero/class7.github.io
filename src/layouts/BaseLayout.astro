---
import RootLayout from "./RootLayout.astro";
const { frontmatter, title } = Astro.props;
---

<RootLayout title={title}>
    <div class="w-full flex flex-1 flex-col justify-center my-32 bg-background">
        <div
            class="w-full py-10 flex justify-center text-3xl font-bold italic underline decoration-solid decoration-ring decoration-4 fontText"
        >
            {title}
        </div>
        <div class="px-3 py-3">
            <slot />
        </div>
    </div>
</RootLayout>
